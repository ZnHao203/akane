config_version=5

[application]

config/name="deskPet"
run/main_scene="res://scene/main_window.tscn"
config/features=PackedStringArray("4.3", "C#", "Forward Plus")
boot_splash/bg_color=Color(0, 0, 0, 0)
boot_splash/show_image=false
config/icon="res://icon.svg"

[display]

window/size/viewport_width=250
window/size/viewport_height=200
window/size/initial_position_type=2
window/size/always_on_top=true
window/size/transparent=true
window/stretch/mode="canvas_items"
window/per_pixel_transparency/allowed=true

[dotnet]

project/assembly_name="deskPet"

[editor_plugins]

enabled=PackedStringArray("res://addons/codeandweb.texturepacker/plugin.cfg")

